{
    "name": "mariuslundgard/php-debug",
    "description": "A debug library for PHP 5.4+.",
    "keywords": ["php", "debug"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius LundgÃ¥rd",
            "email": "studio@mariuslundgard.com"
        }
    ],
    "bin": [
        "bin/pdebug"
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "src/common.php"
        ],
        "psr-0": {
            "Debug": "src/"
        }
    },
    "require": {
        "php": ">=5.4.13",
        "mariuslundgard/php-util": "dev-develop",
        "mariuslundgard/php-core": "dev-develop",
        "mariuslundgard/php-event": "dev-develop",
        "mariuslundgard/php-socket": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phploc/phploc": "*",
        "mayflower/php-codebrowser": "~1.1",
        "phpunit/php-invoker": "dev-master",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "~1.4",
        "sebastian/finder-facade": "dev-master",
        "theseer/fdomdocument": "dev-master",
        "squizlabs/php_codesniffer": "~1.4",
        "nette/nette": "~2.1.3",
        "apigen/apigen": "~2.8",
        "satooshi/php-coveralls": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mariuslundgard/php-event"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mariuslundgard/php-socket"
        },
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ]
}